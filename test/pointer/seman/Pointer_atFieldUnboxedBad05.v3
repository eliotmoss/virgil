// Disallow pointer to *packed* field of a local variable
type V(x: u4, y: u4) #packing 0b_xxxxyyyy;

type V(x: int);
class C {
	var v: V;
	new(i: int) {
		v = V(i, 3);
	}
}

def main() -> int {
	var c = C.new(13);
	var p = Pointer.atField(c.v.x);
	p.store<int>(17);
	return c.v.x;
}
