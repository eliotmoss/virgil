// A test of more complex type recursion

class A {
	var v: int;
	var b: B;
	new(v, b) { }
}

class B {
	var v: int;
	var a: A;
	new(v, a) { }
}

def main(args: Array<string>) -> int {
	var a: A;
	for (i < args.length) {
		var b = B.new(i, a);
		a = A.new(i+1, b);
	}
	var j = 0;
	while (a != null) {
		j += a.v + a.b.v;
		a = a.b.a;
	}
	return j;
}
